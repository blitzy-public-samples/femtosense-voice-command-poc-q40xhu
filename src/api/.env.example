# This file serves as a template for required environment variables.
# Copy this file to .env and replace the placeholder values with actual configuration.

# API Keys
GPT_API_KEY=your_gpt_api_key_here
NARAKEET_API_KEY=your_narakeet_api_key_here

# API Timeouts (in milliseconds)
GPT_API_TIMEOUT=30000
NARAKEET_API_TIMEOUT=30000

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=femtosense-voice-commands

# Application Settings
LOG_LEVEL=info
MAX_BATCH_SIZE=100
RETRY_ATTEMPTS=3

# Security Note: This file is a template and should never contain actual secret values.
# Ensure that your .env file is listed in .gitignore to prevent accidental commits of secrets.